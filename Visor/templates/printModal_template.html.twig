<div id="printModal" class="printModal modal fade" role="dialog">
        <!-- Modal content-->
        <div class="modal-content container">
            <div class="modal-header col-lg-12">
                <h4 class="modal-title">Imprimir Visor</h4>
            </div>
            <div class="modal-body col-lg-12">
                <div class="leftSidePrintModal col-lg-4">

                    <h3>Seleccione informe:</h3>
                    <select class="reportsAvailable col-lg-12">
                    </select>

                    <div class="horizontalPrintSeparator col-lg-12"></div>
                    <div id="title" class="col-lg-12">
                        <h3 class="col-lg-11">Título del informe</h3>
                        <div class="col-lg-1 leftSideCheckbox">
                            <input type="checkbox" checked/>
                        </div>
                        <input type="text" class="form-control" placeholder="Título del informe">
                    </div>
                    <div id="description" class="col-lg-12">
                        <h3 class="col-lg-11">Descripción</h3>

                        <div class="col-lg-1 leftSideCheckbox">
                            <input type="checkbox" checked/>
                        </div>

                        <textarea id="descriptionTextarea" class="form-control" rows="3"></textarea>
                    </div>
                    <div id="modalLegend" class="col-lg-12">
                        <h3 class="col-lg-11">Leyenda</h3>

                        <div class="col-lg-1 leftSideCheckbox">
                            <input type="checkbox" checked/>
                        </div>
                    </div>
                    <div id="modalDatatables" class="col-lg-12">
                        <h3 class="col-lg-11">Tablas de información</h3>

                        <div class="col-lg-1 leftSideCheckbox">
                            <input type="checkbox" checked />
                        </div>
                    </div>
                </div>

                <div class="verticalPrintSeparator col-lg-1">
                    <div class=""></div>
                </div>

                <div class="rightSidePrintModal col-lg-7">
                    <div id="previewCanvas"><iframe></iframe></div>
                </div>
            </div>
            <div class="modal-footer col-lg-12">
                <button type="button" id="printModalButton" class="btn btn-success">Descargar</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancelar</button>
            </div>
        </div>
</div>


